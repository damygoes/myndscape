{
  "expo": {
    "name": "reflect",
    "slug": "reflect",
    "scheme": "ai.reflect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0C0E10"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.reflect"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0C0E10"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.reflect",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "ai.reflect",
            "host": "auth",
            "pathPrefix": "/callback"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ]
  }
}
